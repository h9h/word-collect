{
	"name": "@h9h/word-collect",
	"version": "1.0.5",
	"description": "Small library to collect words, optionally using a stemmer",
	"main": "./dist/index.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/h9h/word-collect.git"
	},
	"keywords": [
		"words",
		"stemmer"
	],
	"author": "h9h",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/h9h/word-collect/issues"
	},
	"homepage": "https://github.com/h9h/word-collect#readme",
	"engine": {
		"node": ">= 0.11"
	},
	"engines": {
		"node": "^8.12.0 || >=9.7.0"
	},
	"scripts": {
		"test": "xo && nyc ava && tsd",
		"debug": "nyc ava --inspect-brk",
		"doc:html": "jsdoc ./index.js",
		"doc:md": "jsdoc2md ./index.js",
		"build": "parcel build ./index.js",
		"release": "env-cmd release-it"
	},
	"xo": {
		"semicolon": false
	},
	"files": [
		"index.js",
		"index.d.ts",
		"dist"
	],
	"devDependencies": {
		"@types/node": "^12.0.7",
		"auto-changelog": "^1.16.1",
		"ava": "^2.1.0",
		"codacy-coverage": "^3.4.0",
		"coveralls": "^3.0.4",
		"env-cmd": "^10.0.1",
		"is-running": "^2.1.0",
		"jsdoc": "^3.6.3",
		"jsdoc-to-markdown": "^5.0.1",
		"nyc": "^14.1.1",
		"p-event": "^4.1.0",
		"parcel-bundler": "^1.12.3",
		"release-it": "^12.4.2",
		"tempfile": "^3.0.0",
		"tsd": "^0.7.3",
		"xo": "^0.24.0"
	},
	"nyc": {
		"exclude": [
			"**/fixtures/**",
			"**/test.js",
			"**/test/**"
		]
	},
	"dependencies": {
		"stemmer": "^1.0.4"
	}
}
